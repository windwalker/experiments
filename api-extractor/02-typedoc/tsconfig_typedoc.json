{
    "compilerOptions": {
      "module": "commonjs",
      "lib": [
        "es6",
        "dom"
      ],
      "noImplicitAny": true,
      "noImplicitThis": true,
      "strictNullChecks": true,
      "strictFunctionTypes": true,
      "baseUrl": "doc/",
      "typeRoots": [
        "doc/"
      ],
      "types": [],
      "noEmit": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": [ "dist"],
    "typedocOptions": {
      "entryPoints": [
        "dist/src/awesome-widgets.d.ts"
      ],
      "disableSources": true,
      "hideGenerator": true,
      "out": "doc"
    }
  }
  